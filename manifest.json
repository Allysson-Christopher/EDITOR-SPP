{
  "manifest_version": 3,
  "name": "Ajuste de Fonte",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "/src/fontSize/fontSize.js",
        "/src/fontSize/observerFontSize.js",
        "/src/fontFamily/fontFamily.js",
        "/src/fontFamily/observerFontFamily.js",
        "/src/identacao/indent.js",
        "/src/identacao/observerIndent.js",
        "main.js"
      ]
    }
  ],
  "permissions": ["scripting"]
}
